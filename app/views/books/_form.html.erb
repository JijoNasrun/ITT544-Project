<%= form_for @book do |f| %>
  <%= render 'errors' %>
  <%= f.label 'name', 'Name' %>
  <%= f.text_field 'name', placeholder: 'Name'%>
 
  <%= f.label 'author', 'Author' %>
  <%= f.text_field 'author', placeholder: 'Author'%>

  <%= f.label 'isbn', 'ISBN' %>
  <%= f.text_field 'isbn', placeholder: 'ISBN'%>

  <%= f.label 'publisher', 'Publisher' %>
  <%= f.text_field 'publisher', placeholder: 'Publisher'%>

  <%= f.submit 'Submit' %>
<% end %>